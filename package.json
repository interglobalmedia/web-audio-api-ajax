{
	"name": "web-audio-api-ajax",
	"version": "1.0.0",
	"description": "Web Audio App Created With Node.js",
	"main": "index.js",
	"scripts": {
		"lint": "eslint src",
		"fix": "npx eslint src --fix",
		"stylelint": "stylelint **/*.{css,scss}",
		"stylelint:fix": "stylelint --fix **/*.{scss}",
		"eslint-check": "eslint src | eslint-config-prettier-check",
		"format": "npm run prettier -- --write",
		"prepare": "husky install",
		"prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
		"validate": "npm run lint && npm run stylelint && npm run prettier",
		"clean": "rm -rf dist/",
		"cleanAudio": "rm dist/Late_Night_Drive.mp3",
		"serve": "webpack-dev-server --mode development --config config/webpack.base.config.js --open --hot --history-api-fallback --node-env development --node-env stag",
		"predeploy": "webpack --mode production --config config/webpack.prod.config.js --env PLATFORM=production --env VERSION=stag --progress",
		"deploy": "gh-pages -d dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interglobalmedia/web-audio-api-ajax.git"
	},
	"author": "Maria D. Campbell",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/interglobalmedia/web-audio-api-ajax/issues"
	},
	"homepage": "https://github.com/interglobalmedia/web-audio-api-ajax#readme",
	"dependencies": {
		"gh-pages": "^3.2.3",
		"webpack-visualizer-plugin2": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@babel/plugin-proposal-export-default-from": "^7.16.7",
		"@babel/plugin-proposal-export-namespace-from": "^7.16.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.16.7",
		"@babel/plugin-proposal-throw-expressions": "^7.16.7",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-modules-commonjs": "^7.16.8",
		"@babel/plugin-transform-object-assign": "^7.16.7",
		"@babel/preset-env": "^7.16.11",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"clean-webpack-plugin": "^4.0.0",
		"compression-webpack-plugin": "^9.2.0",
		"copy-webpack-plugin": "^10.2.1",
		"css-loader": "^6.5.1",
		"eslint": "^8.7.0",
		"eslint-cli": "^1.1.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-webpack-plugin": "^3.1.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"htmlhint": "^1.1.1",
		"husky": "^7.0.4",
		"mini-css-extract-plugin": "^2.5.2",
		"node-sass": "^7.0.1",
		"optimize-css-assets-webpack-plugin": "^6.0.1",
		"prettier": "^2.5.1",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^5.67.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.7.3",
		"webpack-manifest-plugin": "^4.1.1",
		"webpack-merge": "^5.8.0"
	}
}
